{
  "name": "technomancy-blog",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "remark . --output --ext md,mdx",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.5",
    "@astrojs/tailwind": "^6.0.2",
    "astro": "^5.13.7",
    "astro-vtbot": "^1.7.9",
    "gray-matter": "^4.0.3",
    "pnpm": "^10.17.0",
    "remark-cli": "^12.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx": "^3.0.1",
    "tailwindcss": "^3.4.1",
    "turbowatch": "^2.29.4",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "daisyui": "^4.7.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "@playwright/test": "^1.48.2",
    "vitest": "^2.0.5",
    "happy-dom": "^15.11.0"
  },
  "remarkConfig": {
    "settings": {
      "bullet": "*"
    },
    "plugins": [
      "remark-frontmatter",
      [
        "remark-mdx",
        {
          "printWidth": 80
        }
      ]
    ]
  }
}